package(default_visibility = ["//visibility:public"])

load("@bazel_skylib//rules:native_binary.bzl", "native_binary")

RESTRICTIONS = [
    "@platforms//os:linux",
    "@platforms//cpu:x86_64",
]

native_binary(
    name = "emulator",
    src = "emulator_main",
    out = "emulator",
    exec_compatible_with = RESTRICTIONS,
)

native_binary(
    name = "emulator_rest",
    src = "gateway_main",
    out = "emulator_rest",
    exec_compatible_with = RESTRICTIONS,
)
